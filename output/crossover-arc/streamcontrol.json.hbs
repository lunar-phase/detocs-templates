---
{
  "gameIds": {
    "gbvs": "GBVS",
    "ggst": "GGST",
    "melty": "MBAACC",
    "mbtl": "MBTL",
    "uni": "UNICLR",
    "dfci": "DFCI",
    "kofxv": "KOFXV"
  }
}
---
{
    "p1Name": "{{state.players.[0].nameAndStatus}}",
    "p1Team": "{{state.players.[0].person.prefix}}",
    "p1Score": "{{state.players.[0].score}}",
    "p2Name": "{{state.players.[1].nameAndStatus}}",
    "p2Team": "{{state.players.[1].person.prefix}}",
    "p2Score": "{{state.players.[1].score}}",
    "round": "{{state.match.name}}",
    {{#if state.game.id}}
        {{#with (lookup userData.gameIds state.game.id) as | mapping |}}
    "game": "{{mapping}}",
        {{else}}
    "game": "MBAACC",
        {{/with}}
    {{else}}
    "game": "MBAACC",
    {{/if}}
    "offline": true,
    "com1Name": "{{state.commentators.[0].prefixedName}}",
    "com1Twitter": "{{state.commentators.[0].person.twitter}}",
    "com2Name": "{{state.commentators.[1].prefixedName}}",
    "com2Twitter": "{{state.commentators.[1].person.twitter}}"
}

